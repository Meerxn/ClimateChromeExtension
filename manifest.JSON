{
    "name" : "obj ext",
     "description" : "my ext",
     "version": "1.0",
     "manifest_version": 2,
      "background":{
          "scripts" : [ "/background.js"]
      },
      "options_page" : "./options.html",
      "browser_action": {
          "default_popup": "popup.html"
      },
      "permissions":[
          "tabs",
          "https://www.google.com/*"
      ],
      "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"


}